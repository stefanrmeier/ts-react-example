{
  "name": "ts-react-example",
  "version": "1.0.0",
  "description": "",
  "repository": "https://github.com/stefanrmeier/ts-react-example",
  "main": "index.js",
  "scripts": {
    "tslint": "./node_modules/.bin/tslint -c tslint.json ts/**/**.ts tss/**/**.tsx",
    "doc": "./node_modules/.bin/typedoc --out ./doc --mode file --ignoreCompilerErrors --readme ./README.md --target ES6 --excludeExternals  --includeDeclarations ./ts/lib/",
    "clean": "rm -rf ./dist/*",
    "build:css": "node-sass --include-path scss --output-style compressed ./scss/main.scss  ./dist/main.css",
    "js:watch": "export NODE_ENV=development  && ./node_modules/.bin/webpack --progress --watch --config ./webpack/webpack.dev.config.js",
    "build:dev": "export NODE_ENV=development  && ./node_modules/.bin/webpack --progress --config ./webpack/webpack.dev.config.js && npm run build:css",
    "build:prod": "export NODE_ENV=production && ./node_modules/.bin/webpack --hide-modules --progress --config ./webpack/webpack.prod.config.js && npm run build:css",
    "build": "npm run build:prod"
  },
  "author": "Stefan Meier",
  "license": "ISC",
